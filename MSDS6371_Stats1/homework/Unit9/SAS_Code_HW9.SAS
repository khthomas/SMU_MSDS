filename ref1 "/folders/myshortcuts/SMU/MSDS6371 - Stats/Unit 8/moneyball.csv";
filename ref2 "/folders/myshortcuts/SMU/MSDS6371 - Stats/Unit9/TEST_DATA.csv";

proc import datafile=ref2 dbms=csv out=work.testscores;
	getnames=yes;
run;

proc import datafile=ref1 dbms=csv out=work.moneyball;
	getnames=yes;
run;

data work.critvalue;
	critVal = quantile("T", 0.975, 28); /*distribution, area, degree of freedom*/
run;

proc glm data=work.moneyball;
	model wins = payroll / solution clparm; /*confidence limit of the betas*/
run;

/*based on the scatter plot generated by the code below there is appears to 
be a linear relationship between science and math scores*/
proc sgplot data=work.testscores;
	scatter x=science y=math;
run;
	
proc glm data=work.testscores;
	model math = science / solution clparm;
run;

data work.critvalue2;
	critVal = quantile("T", 0.975, 198); /*distribution, area, degree of freedom*/
run;

data work.critvalue3;
	critVal = quantile("T", 0.995, 198);
run;

proc glm data=work.testscores alpha=0.01;
	model math = science / solution clparm;
run;
